{
  "version": "1.0.0-*",
  "testRunner": "xunit",
  "buildOptions": {
    "debugType": "portable",
    "warningsAsErrors": true,
    "copyToOutput": {
      "include": [ "xunit.runner.json" ]
    }
  },
  "dependencies": {
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "Infra.Core":
    {
      "target": "project"
    },
    "Infra.EF6": {
      "target": "project"
    },
    "Shouldly": "2.8.2",
    "EntityFramework": "6.1.3",
    "Moq": "4.5.30",
    "xunit": "2.2.0",
    "xunit.extensibility.core": "2.2.0",
    "xunit.extensibility.execution": "2.2.0",
    "xunit.runner.visualstudio": "2.2.0"
  },

  "frameworks": {
    "NET46": {
      "buildOptions": {
        "define": [ "NET46" ]
      },
      "frameworkAssemblies": {
        "System.Threading.Tasks": {
          "type": "build"
        },
        "System.Runtime": {
          "type": "build"
        }
      }
    }
  }
}