

@using Borg.Framework.GateKeeping.Account
@model Borg.Framework.GateKeeping.Account.LogoutViewModel



@section contentheader
{
    <section class="content-header">

        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>
}


<div class="row">
    <div class="col-md-4 col-md-offset-4">
        <div class="box box-default">
            <form asp-action="Logout">
                <div class="box-header">
                    <h3 class="box-title">@Content.Page.Title</h3>
                </div>
                <div class="box-body">
                    <input type="hidden" name="logoutId" value="@Model.LogoutId"/>
                    <div class="box-body">
                        Realy logout?
                    </div>
                </div>
                <div class="box-footer">
                    <button class="btn btn-primary pull-right" type="submit">Yes</button>
                </div>
            </form>
        </div>
    </div>
</div>
